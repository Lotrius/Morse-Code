{
    "extends": ["react-app", "airbnb", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
      "sourceType": "module"
  },
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      "no-console": "warn"
      // "func-names": "off",
      // "no-process-exit": "off",
      // "object-shorthand": "off",
      // "class-methods-use-this": "off"
    }
  }
